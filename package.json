    "webpack-manifest-plugin": "^1.1.0"
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "compression": "^1.7.2",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.2.0",
    "react-css-modules": "^4.7.1",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.6.0",
    "redux-connect": "^7.0.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.5.0",
    "whs": "2.1.9"
  }
}
